{
    "manifest_version" : 2,
    "name": "BlueRay Filter",
    "version": "1.0",
    "content_scripts":[
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["scripts/content_scripts.js"]
        }
    ],
    "background": {
        "scripts": ["scripts/background_scripts.js"]
    },
    "browser_action":{
        "default_icon": "img.png"
    },
    "permissions": [
        "activeTab"
    ]
}